<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!--如果有多个item，那么程序将自动从上到下进行匹配，最先匹配的将得到应用。-->

    <!--  被按下的状态 -->
    <item android:drawable="@android:color/holo_blue_dark" android:state_pressed="true"></item>
    <!--可被点击 状态-->
    <item android:drawable="@android:color/darker_gray" android:state_enabled="false"></item>
    <!--其他状态-->
    <item android:drawable="@android:color/holo_green_light" android:state_enabled="true"></item>

</selector>